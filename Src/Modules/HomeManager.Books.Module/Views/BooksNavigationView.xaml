<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HomeManager.Books.Module.Views"
             xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
             xmlns:prism="http://prismlibrary.com/"
             xmlns:views="clr-namespace:HomeManager.Books.Module.Views"
             xmlns:dxnav="http://schemas.devexpress.com/winfx/2008/xaml/navigation" x:Class="HomeManager.Books.Module.Views.BooksNavigationView"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             prism:ViewModelLocator.AutoWireViewModel="True">
    <x:Code>
        <![CDATA[
            public BooksNavigationView()
            {
                InitializeComponent();
            }
        ]]>
    </x:Code>

    <Grid>
        <dxnav:TileNavPane>
            <dxnav:TileNavPane.NavButtons>
                <dxnav:NavButton Content="Books" IsMain="False"/>
                <dxnav:NavButton Content="Authors" IsMain="False"/>
                <dxnav:NavButton Content="Genres" IsMain="False"/>
            </dxnav:TileNavPane.NavButtons>
        </dxnav:TileNavPane>
    </Grid>
</UserControl>
